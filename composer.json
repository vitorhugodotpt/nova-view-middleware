{
    "name": "vitorhugodotpt/nova-view-middleware",
    "description": "Middleware to control visibility of index of one Resource in Laravel Nova",
    "keywords": [
        "vitorhugodotpt",
        "nova-view-middleware",
        "laravel",
        "nova"
    ],
    "homepage": "https://github.com/vitorhugodotpt/nova-view-middleware",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Vitor Hugo",
            "email": "vhugo@vitorhugo.pt"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Vitorhugodotpt\\NovaViewMiddleware\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vitorhugodotpt\\NovaViewMiddleware\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vitorhugodotpt\\NovaViewMiddleware\\NovaViewMiddlewareServiceProvider"
            ]
        }
    }
}
